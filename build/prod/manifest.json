{
	"manifest_version": 3,
	"name": "Yu Dereader - Dinosaur Comics Fun Times!",
	"short_name": "Yu Dereader",
	"version": "2.3.1",
	"description": "Yu Dereader reveals Dinosaur Comics easter eggs for you, the reader",
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"all_frames": false,
			"js": [
				"yu.js"
			],
			"css": [
				"dereader.css"
			],
			"matches": [
				"http://*.qwantz.com/*",
				"https://*.qwantz.com/*"
			]
		}
	],
	"icons": {
		"128": "eye-con.png"
	},
	"web_accessible_resources": [
		{
			"resources": [
				"eye-con.png",
				"eye-smooth.png"
			],
			"matches": [
				"<all_urls>"
			]
		}
	]
}